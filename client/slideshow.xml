<Slideshow>
    <Title>Slideshow Example</Title>
    <Description>An AppleDeck slideshow example.</Description>
    <Authors>
        <Author>
            <Name>Mesure73L</Name>
            <URL>https://github.com/Mesure73L</URL>
        </Author>
    </Authors>
    <License>CC0-1.0 Universal</License>
    <Size>
        <Width>1920</Width>
        <Height>1080</Height>
    </Size>
    <Slides>
        <Slide>
            <Title>Title Slide</Title>
            <SpeakerNotes>This is the title slide.\nTalk about title slide things.</SpeakerNotes>
            <BackgroundColor>#9E49C8</BackgroundColor>
            <Elements>
                <Element>
                    <Type>Static</Type>
                    <Template>Text</Template>
                    <Text>Title Slide</Text>
                    <Color>white</Color>
                    <FontFamily>Itim</FontFamily>
                    <FontSize>100</FontSize>
                    <Position>
                        <X>Center</X>
                        <Y>Center</Y>
                        <Rotation>30</Rotation>
                    </Position>
                </Element>
                <Element>
                    <Type>Static</Type>
                    <Template>Image</Template>
                    <Source>logo</Source>
                    <Size>
                        <Width>500</Width>
                        <Height>500</Height>
                    </Size>
                    <Position>
                        <X>100</X>
                        <Y>-100</Y>
                        <Rotation>50</Rotation>
                    </Position>
                </Element>
                <Element>
                    <Type>Interactive</Type>
                    <Template>Button</Template>
                    <Text>Click me</Text>
                    <Color>#FF0000</Color>
                    <BackgroundColor>#000000</BackgroundColor>
                    <Border>
                        <Size>3</Size>
                        <Style>solid</Style>
                        <Color>#FFFFFF</Color>
                    </Border>
                    <Hover>
                        <Color>#000000</Color>
                        <BackgroundColor>#FFFFFF</BackgroundColor>
                        <Border>
                            <Size>3</Size>
                            <Style>solid</Style>
                            <Color>#000000</Color>
                        </Border>
                    </Hover>
                    <FontFamily>Roboto Mono</FontFamily>
                    <FontSize>50</FontSize>
                    <Size>
                        <Width>500</Width>
                        <Height>100</Height>
                    </Size>
                    <Position>
                        <X>Center</X>
                        <Y>Center</Y>
                        <Rotation>0</Rotation>
                    </Position>
                </Element>
            </Elements>
        </Slide>
        <Slide>
            <Title>Slide 2</Title>
            <SpeakerNotes>This is the second slide.\nTalk about second slide things.</SpeakerNotes>
            <BackgroundColor>#000000</BackgroundColor>
            <Elements>
                <Element>
                    <Type>Static</Type>
                    <Template>Text</Template>
                    <Text>Second Slide</Text>
                    <Color>white</Color>
                    <FontFamily>Roboto Mono</FontFamily>
                    <FontSize>30</FontSize>
                    <Position>
                        <X>100</X>
                        <Y>-50</Y>
                        <Rotation>0</Rotation>
                    </Position>
                </Element>
            </Elements>
        </Slide>
    </Slides>
</Slideshow>
